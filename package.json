{
  "name": "sevenzip-node",
  "version": "1.0.1",
  "description": "7-Zip native wrapper around 7z.dll",
  "homepage": "https://github.com/SteezCram/sevenzip-node",
  "repository": {
    "type": "git",
    "url": "git://github.com/SteezCram/sevenzip-node.git"
  },
  "binary": {
    "module_name": "sevenzip-node",
    "module_path": "./lib/{platform}/{arch}",
    "host": "undefined"
  },
  "main": "index.js",
  "gypfile": true,
  "scripts": {
    "start": "node index.js",
    "test": "mocha",
    "build": "node-pre-gyp build --build-from-source",
    "clean": "node-gyp clean",
    "install": "node-pre-gyp install --fallback-to-build"
  },
  "author": "Thomas Croizet (Steez)",
  "license": "MIT",
  "devDependencies": {
    "mocha": "^9.0.1",
    "node-gyp": "^8.0.0"
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.5",
    "node-addon-api": "^4.0.0"
  },
  "keywords": [
    "7z",
    "7za",
    "sevenzip",
    "LZMA",
    "LZMA2",
    "compress"
  ]
}
